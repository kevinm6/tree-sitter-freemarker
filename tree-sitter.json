{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "freemarker",
      "camelcase": "Freemarker",
      "title": "Freemarker",
      "scope": "source.freemarker",
      "file-types": [
        "freemarker"
      ],
      "injection-regex": "^freemarker$",
      "class-name": "TreeSitterFreemarker"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Freemarker grammar for tree-sitter",
    "authors": [
      {
        "name": "KevinM6",
        "url": "https://kevinm6.github.io/"
      }
    ],
    "links": {
      "repository": "https://github.com/kevinm6/tree-sitter-freemarker"
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "node": false,
    "python": false,
    "rust": false,
    "swift": false,
    "zig": false
  }
}
