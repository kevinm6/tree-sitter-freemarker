{
  "grammars": [
    {
      "name": "freemarker",
      "camelcase": "Freemarker",
      "scope": "source.freemarker",
      "path": ".",
      "file-types": ["freemarker"],
      "injection-regex": "^(freemarker)|(Freemarker)|(FreeMarker)$",
      "first-line-regex": "^<#ftl ns_prefixes",
      "highlights": ["queries/highlights.scm"]
    }
  ],
  "metadata": {
    "version": "1.5.0",
    "license": "MIt",
    "description": "Apache Freemarker",
    "authors": [
      {
        "name": "Kevin M",
        "email": ""
      }
    ],
    "links": {
      "repository": "https://github.com/kevinm6/tree-sitter-freemarker"
    }
  },
  "bindings": {
    "c": true,
    "node": true
  }
}
